spring:
  application:
    name: cube-api
server:
  port: 9001


management:
  endpoints:
    web:
      exposure:
        include: '*'
  endpoint:
    health:
      show-details: always
    shutdown:
      enabled: true
eureka:
  instance:
    preferIpAddress: false
    hostname: localhost
  client:
    service-url:
      defaultZone: http://localhost:8761/eureka/
